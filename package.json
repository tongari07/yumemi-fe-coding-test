{
  "name": "yumemi-fe-coding-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare-env": "vercel env pull",
    "dev": "vite",
    "start": "vercel dev",
    "build": "tsc && vite build",
    "lint": "eslint . --cache --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:run": "VITE_API_BASE_URL='http://localhost:3000' vitest run",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vercel/node": "^2.15.9",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.7.3",
    "swr": "^2.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4",
    "vercel": "^31.2.3",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown .",
    "*.{ts,tsx}": "pnpm lint"
  },
  "pnpm": {
    "overrides": {
      "@vercel/node>ts-node": "10.9.1",
      "@vercel/node>typescript": "5.0.4"
    }
  }
}
